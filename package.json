{
  "name": "rxfire-admin",
  "version": "1.0.4",
  "private": false,
  "description": "Firebase Admin JavaScript library RxJS",
  "author": {
    "email": "aquoc.hue@outlook.com",
    "name": "anhquoctran",
    "url": "https://github.com/anhquoctran"
  },
  "license": "Apache-2.0",
  "homepage": "https://firebase.google.com/",
  "sideEffects": false,
  "main": "index.ts",
  "keywords": [
    "authentication",
    "database",
    "Firebase",
    "firebase",
    "realtime",
    "storage",
    "rxjs",
    "notifications"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anhquoctran/rxfire"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.js '**/*.ts' --ignore-path './.gitignore'",
    "lint:fix": "eslint --fix -c .eslintrc.js '**/*.ts' --ignore-path './.gitignore'",
    "build": "npm run build:clean && tsc",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build:docs": "cp README.md ./dist/ && cp -r ./docs ./dist/",
    "dev": "rollup -c -w",
    "echo:chrome": "echo 'Open Chrome DevTools: \nchrome://inspect/#devices'",
    "test": "FIREBASE_CLI_PREVIEWS=storageemulator firebase emulators:exec \"jest --detectOpenHandles\" --project=rxfire-test-c497c ",
    "test:debug": "yarn echo:chrome && FIREBASE_CLI_PREVIEWS=storageemulator firebase emulators:exec ./test-debug.sh --project=rxfire-test-c497c"
  },
  "dependencies": {
    "firebase": "^9.0.0",
    "firebase-admin": "^9.11.1",
    "firebase-tools": "^9.10.2",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.1",
    "xhr2": "^0.2.1"
  },
  "peerDependencies": {
    "firebase": "^9.0.2",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-jest": "^26.6.3",
    "cross-fetch": "^3.1.4",
    "eslint": "^7.17.0",
    "eslint-config-google": "^0.14.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "tslib": "^2.3.1",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "docs/**/*"
  ]
}
